{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Internet Archive This",
  "description": "Submits the current tab to the Internet Archive (must be publicly accessible).",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [ "http://*/*", "https://*/*" ],
  "incognito": "split",
  "offline_enabled": false,
  "homepage_url": "https://www.aeyoun.com/projects/extensions/internte-archive-this.html",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Submit page to Internet Archive",
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    }
  }
}
